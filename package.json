{
   "name": "pony-run",
   "description": "A simple dino-like HTML5-canvas TS game with ponies",
   "repository": "git@github.com:ponydevs/ponyrun.git",
   "license": "ISC",
   "scripts": {
      "build": "parcel build src/page.html -d public -o index.html"
   },
   "ava": {
      "extensions": [
         "ts"
      ],
      "require": [
         "ts-node/register"
      ],
      "files": [
         "src/**/*.test.ts"
      ]
   },
   "devDependencies": {
      "@types/node": "^13.9.1",
      "ava": "^3.8.1",
      "prettier": "^1.19.1",
      "ts-node": "^8.6.2",
      "typescript": "^3.8.3"
   },
   "dependencies": {
      "parcel": "^1.12.4",
      "rxjs": "^7.0.0-beta.0"
   }
}
